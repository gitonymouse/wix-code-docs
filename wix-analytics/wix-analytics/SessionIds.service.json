          {
            "title": "filterSessions example for exporting from backend code",
            "body": [
              "import { sessionIds } from 'wix-analytics';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedFilterSessions = elevate(sessionIds.filterSessions);",
              "",
              "export const filterSessions = webMethod(",
              "  Permissions.Anyone,",
              "  async (options) => {",
              "    try {",
              "      const result = await elevatedFilterSessions(options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }

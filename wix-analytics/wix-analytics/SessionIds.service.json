        "examples": [
          {
            "title": "filterSessions example for dashboard page code",
            "body": [
              "import { sessionIds } from 'wix-analytics';",
              "",
              "async function filterSessions(options) {",
              "  try {",
              "    const result = await sessionIds.filterSessions(options);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",

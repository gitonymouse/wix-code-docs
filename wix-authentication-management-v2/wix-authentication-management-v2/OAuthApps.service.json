      "requiredFields": [
        "oAuthApp",
        "oAuthApp.name"
      ],
      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-authentication-management-v2.OAuthApps.OAuthApp"
                }
              ]
            }
          }
        ],
        "doc": "Created OAuth app info.\n"
      },
      "docs": {
        "summary": "Creates a new OAuth app for a Wix Headless client.",
        "description": [
          "> **Note:** The OAuth app secret is returned only when creating the OAuth app, and can't be retrieved later. Store the secret in a secure location."
        ],
        "examples": [
          {
            "title": "createOAuthApp example",
            "body": [
              "import { oAuthApps } from 'wix-authentication-management.v2';",
              "  ",
              " async function createOAuthApp(oAuthApp) {",
              "   try {",
              "     const result = await oAuthApps.createOAuthApp(oAuthApp);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ]
    },

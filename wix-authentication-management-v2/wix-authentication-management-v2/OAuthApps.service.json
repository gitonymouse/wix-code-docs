      "name": "createOAuthApp",
      "params": [
        {
          "name": "oAuthApp",
          "type": [
            {
              "referenceType": "wix-authentication-management-v2.OAuthApps.OAuthApp"
            }
          ],
          "doc": "OAuth app to create.",
          "required": true

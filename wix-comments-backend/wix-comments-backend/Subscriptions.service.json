        "doc": null
      },
      "docs": {
        "summary": "Gets subscription and its status",
        "description": [],
        "examples": [
          {
            "title": "getSubscription example for dashboard page code",
            "body": [
              "import { subscriptions } from 'wix-comments-backend';",
              "",
              "async function getSubscription(subscription) {",
              "  try {",
              "    const result = await subscriptions.getSubscription(subscription);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "getSubscription example for exporting from backend code",
            "body": [
              "import { subscriptions } from 'wix-comments-backend';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedGetSubscription = elevate(subscriptions.getSubscription);",
              "",
              "export const getSubscription = webMethod(",
              "  Permissions.Anyone,",
              "  async (subscription) => {",
              "    try {",
              "      const result = await elevatedGetSubscription(subscription);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },

        "examples": [
          {
            "title": "assignRole example",
            "body": [
              "import { authorization } from 'wix-members.v2';",
              "",
              "async function assignRole(memberId, roleKey) {",
              "  try {",
              "    const result = await authorization.assignRole(memberId, roleKey);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "assignRole example for exporting from backend code",
            "body": [
              "import { authorization } from 'wix-members.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "",
              "export const assignRole = webMethod(",
              "  Permissions.Anyone,",
              "  async (memberId, roleKey) => {",
              "    try {",
              "      const result = await authorization.assignRole(memberId, roleKey);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]

      "docs": {
        "description": [],
        "examples": [
          {
            "title": "queryParticipants example for dashboard page code",
            "body": [
              "import { participant } from 'wix-livevideo-backend';",
              "",
              "async function queryParticipants() {",
              "  const { items } = participant.queryParticipants().find();",
              "}",
              ""
            ]
          },
          {
            "title": "queryParticipants example for exporting from backend code",
            "body": [
              "import { participant } from 'wix-livevideo-backend';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedQueryParticipants = elevate(participant.queryParticipants);",
              "",
              "export const queryParticipants = webMethod(",
              "  Permissions.Anyone,",
              "  async (options) => {",
              "    try {",
              "      const result = await elevatedQueryParticipants(options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },
      "isVeloEvent": false,

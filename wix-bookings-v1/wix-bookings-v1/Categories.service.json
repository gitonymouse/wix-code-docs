        "doc": "Category information.\n"
      },
      "docs": {
        "summary": "Creates a new category.",
        "description": [],
        "examples": [
          {
            "title": "createCategory example for dashboard page code",
            "body": [
              "import { categories } from 'wix-bookings.v1';",
              "",
              "async function createCategory(category) {",
              "  try {",
              "    const result = await categories.createCategory(category);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "createCategory example for exporting from backend code",
            "body": [
              "import { categories } from 'wix-bookings.v1';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedCreateCategory = elevate(categories.createCategory);",
              "",
              "export const createCategory = webMethod(",
              "  Permissions.Anyone,",
              "  async (category) => {",
              "    try {",
              "      const result = await elevatedCreateCategory(category);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "createCategory",
      "isAdminMethod": true
    },

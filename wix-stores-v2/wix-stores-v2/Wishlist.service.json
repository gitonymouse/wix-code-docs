      },
      "docs": {
        "summary": "Get wishlist by id",
        "description": [],
        "examples": [
          {
            "title": "getWishlistById example for dashboard page code",
            "body": [
              "import { wishlist } from 'wix-stores.v2';",
              "",
              "async function getWishlistById(id, options) {",
              "  try {",
              "    const result = await wishlist.getWishlistById(id, options);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "getWishlistById example for exporting from backend code",
            "body": [
              "import { wishlist } from 'wix-stores.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedGetWishlistById = elevate(wishlist.getWishlistById);",
              "",
              "export const getWishlistById = webMethod(",
              "  Permissions.Anyone,",
              "  async (id, options) => {",
              "    try {",
              "      const result = await elevatedGetWishlistById(id, options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "getWishlistById",
      "isAdminMethod": true
    }

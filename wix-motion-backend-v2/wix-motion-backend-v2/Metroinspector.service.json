        "examples": [
          {
            "title": "echo example for dashboard page code",
            "body": [
              "import { metroinspector } from 'wix-motion-backend.v2';",
              "",
              "async function echo(arg1, options) {",
              "  try {",
              "    const result = await metroinspector.echo(arg1, options);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "echo example for exporting from backend code",
            "body": [
              "import { metroinspector } from 'wix-motion-backend.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedEcho = elevate(metroinspector.echo);",
              "",
              "export const echo = webMethod(",
              "  Permissions.Anyone,",
              "  async (arg1, options) => {",
              "    try {",
              "      const result = await elevatedEcho(arg1, options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },

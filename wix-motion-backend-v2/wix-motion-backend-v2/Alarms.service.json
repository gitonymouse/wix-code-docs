      "docs": {
        "summary": "sets up an alarm after {seconds}",
        "description": [],
        "examples": [
          {
            "title": "alarm example for dashboard page code",
            "body": [
              "import { alarms } from 'wix-motion-backend.v2';",
              "",
              "async function alarm(seconds, options) {",
              "  try {",
              "    const result = await alarms.alarm(seconds, options);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "alarm example for exporting from backend code",
            "body": [
              "import { alarms } from 'wix-motion-backend.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedAlarm = elevate(alarms.alarm);",
              "",
              "export const alarm = webMethod(",
              "  Permissions.Anyone,",
              "  async (seconds, options) => {",
              "    try {",
              "      const result = await elevatedAlarm(seconds, options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },
      "isVeloEvent": false,

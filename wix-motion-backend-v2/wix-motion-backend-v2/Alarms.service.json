        "examples": [
          {
            "title": "updateAlarm example for dashboard page code",
            "body": [
              "import { alarms } from 'wix-motion-backend.v2';",
              "",
              "async function updateAlarm(id, alarm, options) {",
              "  try {",
              "    const result = await alarms.updateAlarm(id, alarm, options);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "updateAlarm example for exporting from backend code",
            "body": [
              "import { alarms } from 'wix-motion-backend.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedUpdateAlarm = elevate(alarms.updateAlarm);",
              "",
              "export const updateAlarm = webMethod(",
              "  Permissions.Anyone,",
              "  async (id, alarm, options) => {",
              "    try {",
              "      const result = await elevatedUpdateAlarm(id, alarm, options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]

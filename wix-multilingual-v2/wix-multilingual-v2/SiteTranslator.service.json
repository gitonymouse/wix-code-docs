            "body": [
              "import { siteTranslator } from 'wix-multilingual.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedGetSiteTranslatablesProperties = elevate(siteTranslator.getSiteTranslatablesProperties);",
              "",
              "export const getSiteTranslatablesProperties = webMethod(",
              "  Permissions.Anyone,",
              "  async (mainLanguage, options) => {",
              "    try {",
              "      const result = await elevatedGetSiteTranslatablesProperties(mainLanguage, options);",
              "      return result;",

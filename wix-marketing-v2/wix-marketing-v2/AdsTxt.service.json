  "operations": [
    {
      "name": "appendAdsTxt",
      "params": [
        {
          "name": "options",
          "type": [
            {
              "referenceType": "wix-marketing-v2.AdsTxt.AppendAdsTxtOptions"
            }
          ],
          "doc": "Options to use when appending the Ads.txt file.",
          "required": false
        }
      ],
      "requiredFields": [],
      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-marketing-v2.AdsTxt.AppendAdsTxtResponse"
                }
              ]
            }
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "Adds `content` to the Ads.txt file.",
        "description": [
          "To reset the Ads.txt file to Wix's default, set `default` to `true` and omit `content`."
        ],
        "examples": [
          {
            "title": "appendAdsTxt example for dashboard page code",
            "body": [
              "import { adsTxt } from 'wix-marketing.v2';",
              "",
              "async function appendAdsTxt(options) {",
              "  try {",
              "    const result = await adsTxt.appendAdsTxt(options);",
              "    return result;",
              "  } catch (error) {",
              "    console.error(error);",
              "    // Handle the error",
              "  }",
              "}",
              ""
            ]
          },
          {
            "title": "appendAdsTxt example for exporting from backend code",
            "body": [
              "import { adsTxt } from 'wix-marketing.v2';",
              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedAppendAdsTxt = elevate(adsTxt.appendAdsTxt);",
              "",
              "export const appendAdsTxt = webMethod(",
              "  Permissions.Anyone,",
              "  async (options) => {",
              "    try {",
              "      const result = await elevatedAppendAdsTxt(options);",
              "      return result;",
              "    } catch (error) {",
              "      console.error(error);",
              "      // Handle the error",
              "    }",
              "  }",
              ");",
              ""
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "appendAdsTxt",
      "isAdminMethod": true
    },

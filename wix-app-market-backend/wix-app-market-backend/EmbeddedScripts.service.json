      "docs": {
        "summary": "Inject a script in an app with an existing embedded script component, with the given parameters. Key names must match exactly to the key names used in the app's embedded script",
        "description": [],
        "examples": [
          {
            "title": "embedScript example",
            "body": [
              "import { embeddedScripts } from 'wix-app-market-backend';",
              "  ",
              " async function embedScript(properties, options) {",
              "   try {",
              "     const result = await embeddedScripts.embedScript(properties, options);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
